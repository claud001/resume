<div class="card">
    {{#each reviewList}}
    <div class="card m-3 p-3">
        <div><strong>{{title.userName}}</strong></div>
        <div>{{title.caption}}</div>
        <div>{{title.date}}</div>
        <hr>
        {{#with reviewBody}}
        <p>{{seller.title}}: {{seller.value}}</p>
        <p>{{rating.title}}: {{rating.value}}</p>
        <p>{{text}}</p>
        <p class="m-0"><strong>{{advantages.title}}:</strong></p>
        <p>{{advantages.text}}</p>
        <p class="m-0"><strong>{{disadvantages.title}}:</strong></p>
        <p>{{disadvantages.text}}</p>
        <div class="gx-2">
            {{#each images}}
            <img class="me-2" src="{{this}}" alt="photos">
            {{/each}}
        </div>
    </div>
    {{#each coments}}
    <div class="card col-11 ms-5 my-2 p-2">
        <p> <strong>{{userName}}</strong></p>
        <div>{{date}}</div>
        <div>{{text}}</div>
    </div>
    {{/each}}
    {{/with}}
    {{/each}}
</div>
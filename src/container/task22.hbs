<main>
    <div class="text-center">
        <h1>{{page_header.title}}</h1>
        <p>{{page_header.description}}</p>
    </div>
    <div class="card">
        <div class="card-body ">
            {{#with section.about_us}}
            <p class="display-6 text-end">{{title_list.main}}</p>
            <h2 class="text-end">{{title_list.sub}}</h2>
            <button class="btn btn-outline-info">{{button.text}}</button>
            {{/with}}
        </div>
    </div>
    <br>
    <br>

    <div class="card">
        {{#with section.advantage}}
        <div class="card-body">
            <div class="display-5">{{title}}</div><br>

            <div class="card">
                <div class="card-body">
                    {{#with efficiency}}
                    <div class="display-4">{{title}}</div>
                    <h6>{{sub_title}}</h6>
                    <p>{{description}}</p>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with innovation}}
                    <div class="text-center">
                        <div class="display-4">{{title}}</div>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>
                    </div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with collaboration}}
                    <div class="text-end">
                        <div class="display-4">{{title}}</div>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>
                    </div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>

        </div>
        {{/with}}
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            <div class="display-5">{{section.agreement.title}}</div>
            <div class="card">
                <div class="card-body">
                    <h5>{{section.agreement.info.sub_title}}</h5>
                    <div class="card">
                        <div class="card-body">
                            <p>{{section.agreement.info.detail.text}}</p>
                            <button class="btn btn-success">{{section.agreement.info.detail.button}}</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</main>
<main class="m-5">
    <br>
    <div class="card">

        <div class="card-body">

            <div class="card">
                <div class="card-body">
                    {{#with heading.main}}
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>
                    <button class="btn btn-primary">
                        {{button_text.solid}}
                    </button>
                    <br>
                    <br>
                    <button class="btn btn-outline-primary">
                        {{button_text.outline}}
                    </button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with heading.second}}
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>
                    <button class="btn btn-secondary">
                        {{button_text.solid}}
                    </button>
                    <br>
                    <br>
                    <button class="btn btn-outline-secondary">
                        {{button_text.outline}}
                    </button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with heading.subblock}}
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>
                    <button class="btn btn-dark">
                        {{button_text.solid}}
                    </button>
                    <br>
                    <br>
                    <button class="btn btn-outline-dark">
                        {{button_text.outline}}
                    </button>
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <div class="display-4">{{title}}</div>
                    <p>{{text}}</p>
                    <button class="btn btn-outline-warning">
                        {{button.text}}
                    </button>
                    {{/with}}
                </div>
            </div>
            <br>
            <h6>{{header_text}}</h6>
            <button class="btn btn-info">{{button.text}}</button>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <p class="text-uppercase">{{title}}</p>
            <p>{{text}}</p>
            <a href="{{button.url}}" class=" btn btn-link">{{button.text}}</a>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                    <ul>
                        <li> <a href="{{url}}">{{text}}</a></li>
                    </ul>
                    {{/each}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
</main>